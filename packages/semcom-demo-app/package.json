{
  "name": "@useid/semcom-demo-app",
  "version": "0.11.0",
  "license": "agpl-3.0",
  "scripts": {
    "start": "http-server -p 4200 --gzip --brotli --proxy http://localhost:4200? dist/",
    "start:dev": "vite",
    "start:sync:dev": "npm run start:dev",
    "build": "rimraf ./dist && vite build --config vite.config.ts && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --silent --runInBand --passWithNoTests",
    "posttest": "jest-coverage-thresholds-bumper --silent --coverage-summary-path coverage-summary.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@useid/inrupt-solid-client": "0.3.0",
    "@useid/semcom-core": "0.11.0",
    "@useid/semcom-sdk": "0.11.0",
    "@useid/ui-transfer-components": "0.6.1",
    "@useid/ui-transfer-theme": "0.6.1",
    "@xstate/inspect": "^0.4.1",
    "cross-env": "^7.0.3",
    "lit-element": "^2.5.1",
    "rx-lit": "^1.0.1",
    "rxjs": "^6.0.0",
    "xstate": "^4.22.0"
  },
  "devDependencies": {
    "@useid/typescript-config": "^0.8.1",
    "@types/jest": "^29.1.3",
    "@types/node": "^18.0.0",
    "eslint": "^7.16.0",
    "http-server": "^0.12.3",
    "jest": "^29.5.0",
    "jest-coverage-thresholds-bumper": "1.0.1",
    "jest-fetch-mock": "^3.0.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/useid/semcom.git",
    "directory": "packages/semcom-demo-app"
  },
  "jest": {
    "preset": "@useid/jest-config",
    "displayName": "demo-app",
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
