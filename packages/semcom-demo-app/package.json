{
  "name": "@digita-ai/semcom-demo-app",
  "version": "0.4.1",
  "scripts": {
    "start": "http-server -p 4200 --gzip --brotli --proxy http://localhost:4200? dist/",
    "start:watch": "vite --config vite.config.ts",
    "build": "rimraf ./dist && vite build --config vite.config.ts && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --silent --runInBand --passWithNoTests",
    "posttest": "jest-coverage-thresholds-bumper --silent --coverage-summary-path coverage-summary.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@digita-ai/semcom-core": "0.4.1",
    "@digita-ai/semcom-sdk": "0.4.1",
    "@inrupt/solid-client": "1.6.1",
    "@inrupt/solid-client-authn-browser": "1.8.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@xstate/inspect": "^0.4.1",
    "bulma": "^0.9.2",
    "lit-element": "^2.5.1",
    "rx-lit": "1.0.1",
    "sass": "^1.35.1",
    "tslib": "^2.1.0",
    "xstate": "^4.20.0",
    "zone.js": "~0.11.3",
    "@digita-ai/ui-transfer-components": "0.6.1",
    "@digita-ai/ui-transfer-theme": "0.6.1"
  },
  "devDependencies": {
    "@digita-ai/typescript-config": "^0.3.4",
    "@ngrx/schematics": "^10.1.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "jest-coverage-thresholds-bumper": "0.0.4",
    "lint-staged": "^10.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "4.1.5",
    "vite": "2.2.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digita-ai/semcom.git",
    "directory": "packages/semcom-demo-app"
  },
  "jest": {
    "preset": "@digita-ai/jest-config",
    "displayName": "demo-app",
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
