@prefix  acl:   <http://www.w3.org/ns/auth/acl#>.
@prefix  foaf:  <http://xmlns.com/foaf/0.1/>.

<#tony>
    a               acl:Authorization;
    acl:agent       <http://localhost:9000/tonypaillard/profile/card#me>;
    acl:accessTo    <./>;
    acl:default     <./>;
    acl:mode        acl:Read,
                    acl:Write.

<#public>
    a               acl:Authorization;
    acl:agentClass  foaf:Agent;
    acl:accessTo    <./>;
    acl:default     <./>;
    acl:mode        acl:Read.
