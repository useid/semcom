{
  "name": "@digita-ai/semcom-components",
  "version": "0.2.1",
  "description": "Collection of Semantic Components",
  "author": "Wouter Termont <wouter.termont@digita.ai>",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cp index.html dist && ./node_modules/.bin/http-server -p 80 --gzip --brotli --proxy 'http://localhost:80?' dist/",
    "serve": "./node_modules/.bin/snowpack dev --polyfill-node",
    "build": "rm -rf ./dist && npm run build:ts",
    "build:ts": "./node_modules/.bin/tsc",
    "build:sp": "./node_modules/.bin/snowpack build --polyfill-node",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint --fix .",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@digita-ai/semcom-core": "0.2.1",
    "@types/rdf-ext": "^1.3.8",
    "@types/rdf-js": "^4.0.1",
    "lit-element": "^2.4.0",
    "rdf-data-factory": "^1.0.4",
    "rdf-ext": "^1.3.0"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "http-server": "^0.12.3",
    "snowpack": "^3.0.11",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digita-ai/semcom.git",
    "directory": "packages/semcom-components"
  },
  "bugs": {
    "url": "https://github.com/digita-ai/semcom/issues"
  }
}
