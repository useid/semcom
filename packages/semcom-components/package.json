{
  "name": "@digita-ai/semcom-components",
  "version": "0.3.4",
  "description": "Collection of Semantic Components",
  "author": "Wouter Termont <wouter.termont@digita.ai>",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cp index.html dist && http-server -p 8080 --gzip --brotli --cors --proxy 'http://localhost:8080?' dist/",
    "serve": "snowpack dev --polyfill-node --open none",
    "build": "rm -rf ./dist && npm run build:sp",
    "build:ts": "tsc",
    "build:sp": "snowpack build --polyfill-node",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "prepare": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.json"
    ],
    "ignorePatterns": "snowpack.config.js"
  },
  "dependencies": {
    "@digita-ai/semcom-core": "0.3.4",
    "@snowpack/plugin-webpack": "^2.3.1",
    "@types/n3": "^1.8.0",
    "@types/rdf-ext": "^1.3.8",
    "@types/rdf-js": "^4.0.1",
    "lit-element": "^2.4.0",
    "n3": "^1.9.0",
    "rdf-data-factory": "^1.0.4",
    "rdf-ext": "^1.3.0"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "http-server": "^0.12.3",
    "lint-staged": "^10.5.4",
    "snowpack": "^3.0.11",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digita-ai/semcom.git",
    "directory": "packages/semcom-components"
  },
  "bugs": {
    "url": "https://github.com/digita-ai/semcom/issues"
  }
}
