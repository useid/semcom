{
  "name": "@useid/semcom-components",
  "version": "0.11.0",
  "description": "Collection of Semantic Components",
  "author": "Wouter Termont <wouter.termont@digita.ai>",
  "main": "./dist/semcom-components.umd.js",
  "module": "./dist/semcom-components.es.js",
  "types": "./dist/index.d.ts",
  "license": "agpl-3.0",
  "exports": {
    ".": {
      "import": "./dist/semcom-components.es.js",
      "require": "./dist/semcom-components.umd.js"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "vite",
    "start:sync:dev": "npm run start:dev",
    "build": "rimraf ./dist && npm run build:bundle && npm run build:ts",
    "build:bundle": "vite build",
    "build:ts": "tsc",
    "build:dev": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "posttest": "jest-coverage-thresholds-bumper --silent --coverage-summary-path coverage-summary.json"
  },
  "dependencies": {
    "@useid/dgt-components": "0.11.1",
    "@useid/dgt-theme": "0.11.1",
    "@useid/dgt-utils": "0.11.1",
    "@useid/semcom-core": "0.11.0",
    "@useid/semcom-sdk": "0.11.0",
    "buffer": "6.0.3",
    "jsbarcode": "^3.11.5",
    "lit-element": "^2.4.0",
    "n3": "1.10.0",
    "rx-lit": "1.0.1",
    "xstate": "^4.26.0"
  },
  "devDependencies": {
    "@useid/typescript-config": "^0.8.1",
    "@types/jest": "^29.1.3",
    "@types/node": "^18.16.5",
    "@types/n3": "1.10.0",
    "@types/rdf-js": "^4.0.1",
    "eslint": "^7.16.0",
    "http-server": "^0.12.3",
    "jest": "^29.5.0",
    "jest-coverage-thresholds-bumper": "1.0.1",
    "lint-staged": "^10.5.4",
    "path": "^0.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/useid/semcom.git",
    "directory": "packages/semcom-components"
  },
  "bugs": {
    "url": "https://github.com/useid/semcom/issues"
  },
  "eslintIgnore": [
    "demo"
  ]
}
