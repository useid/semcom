{
  "name": "@useid/semcom-node",
  "version": "0.11.0",
  "description": "> TODO: description",
  "author": "Wouter Janssens <wouter@digita.ai>",
  "homepage": "https://github.com/useid/semcom/tree/master/packages/semcom-node#readme",
  "license": "agpl-3.0",
  "main": "./dist/public-api.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist",
    ".componentsignore",
    "config"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/useid/semcom.git",
    "directory": "packages/semcom-node"
  },
  "types": "./dist/public-api.d.ts",
  "bin": {
    "semcom-node": "./bin/server.js"
  },
  "scripts": {
    "start": "node bin/server.js --no-sync",
    "start:dev": "node bin/server.js -p 3000 -c config/config-default.json",
    "start:sync:dev": "node bin/synchronizing-servers.js -y",
    "build": "rimraf ./dist && npm run build:ts && npm run build:components",
    "build:ts": "tsc",
    "build:components": "componentsjs-generator --typeScopedContexts -s lib -c dist/components -i .componentsignore",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "test": "jest",
    "test:ci": "jest --runInBand --silent",
    "posttest": "jest-coverage-thresholds-bumper --silent --coverage-summary-path coverage-summary.json"
  },
  "bugs": {
    "url": "https://github.com/useid/semcom/issues"
  },
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@useid/semcom-node",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@useid/semcom-node/^0.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@useid/semcom-node/^0.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@useid/semcom-node/^0.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@useid/semcom-node/^0.0.0/dist/": "dist/"
  },
  "engines": {
    "node": ">=12.17 <15"
  },
  "dependencies": {
    "@useid/handlersjs-core": "^0.3.4",
    "@useid/handlersjs-http": "^0.3.4",
    "@useid/semcom-core": "0.11.0",
    "componentsjs": "^5.0.0",
    "n3": "^1.6.3",
    "node-fetch": "^2.6.1",
    "rdf-serialize": "^1.0.1",
    "rxjs": "6.3.3",
    "semver": "^7.3.5",
    "streamify-array": "^1.0.1",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@useid/typescript-config": "^0.8.1",
    "@types/jest": "^29.1.3",
    "@types/n3": "1.10.0",
    "@types/node": "^18.16.5",
    "@types/node-fetch": "^2.5.12",
    "@types/streamify-array": "^1.0.0",
    "@types/supertest": "^2.0.10",
    "@types/yargs": "^17.0.2",
    "componentsjs-generator": "^3.0.3",
    "eslint": "^7.16.0",
    "jest": "^29.5.0",
    "jest-coverage-thresholds-bumper": "1.0.1",
    "lint-staged": "^10.5.4",
    "rimraf": "^2.6.2",
    "supertest": "^6.0.1",
    "ts-jest": "^29.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^5.0.4"
  },
  "eslintIgnore": [
    "bin"
  ],
  "jest": {
    "preset": "@useid/jest-config",
    "displayName": "node",
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/.*\\.conf(ig)?.ts",
      "<rootDir>/lib/public-api.ts",
      "<rootDir>/lib/main.ts",
      "<rootDir>/bin"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 64.56,
        "functions": 81.93,
        "lines": 79.53,
        "statements": 79.22
      }
    }
  }
}
