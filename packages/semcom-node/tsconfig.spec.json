{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": false,
    "strict": false,
    "target": "es2020",
    "types": ["jest", "node"],
    "allowJs": true,
  },
  "files": [
    "./jest.config.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "include": [
    "**/*.spec.ts"
  ]
}
